    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="janeiro, fevereiro, marco, abril, media, total" type="Real" array="False" size=""/>
            <output expression="&quot;Digite quanto foi vendido em janeiro : &quot;" newline="False"/>
            <input variable="janeiro"/>
            <output expression="&quot;Digite quanto foi vendido em fevereiro : &quot;" newline="False"/>
