            <input variable="marco"/>
            <output expression="&quot;Digite quanto foi vendido em abril: &quot;" newline="False"/>
            <input variable="abril"/>
            <assign variable="media" expression="(janeiro + fevereiro + marco + abril) / 4"/>
            <assign variable="total" expression="janeiro + fevereiro + marco + abril"/>
            <output expression="&quot;A media das vendas R$: &quot; &amp; media" newline="True"/>
            <if expression="media &gt;= 5000.00">
                <then>
                    <output expression="&quot;Parab&#233;ns voc&#234; recer&#225; abono de 10%&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Voc&#234; receber&#225; apenas 3%&quot;" newline="True"/>
                </else>
            </if>
            <output expression="&quot;Total das vendas R$: &quot; &amp; total" newline="True"/>
        </body>
